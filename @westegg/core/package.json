{
  "name": "@westegg/core",
  "version": "0.0.2-0",
  "license": "MIT",
  "description": "Westegg Core Gatsby, Typescript, React Components",
  "author": "Andrew Alexander (adrw)",
  "main": "lib/core/core.js",
  "module": "lib/core/core.js",
  "typings": "lib/core/index.d.ts",
  "engines": {
    "node": ">=7.8.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:adrw/westegg.git"
  },
  "scripts": {
    "build": "npm run-script lint && npm run-script lib",
    "clean": "rm -rf lib",
    "ci-build": "npm run-script clean && npm run-script lib",
    "lib": "webpack",
    "lint": "prettier --write --config package.json \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "prepare": "npm run-script ci-build",
    "reinstall": "rm -rf node_modules && npm install",
    "start": "npm run-script lint && cross-env NODE_ENV=development webpack-dev-server",
    "test": "echo no tests"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "bulma": "^0.7.4",
    "gatsby": "^2.3.3",
    "gatsby-image": "^2.0.35",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-netlify-cms": "^3.0.17",
    "gatsby-plugin-purgecss": "^3.1.1",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms": "^2.6.1",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "uuid": "^3.2.2",
    "webpack-sources": "^1.3.0"
  },
  "devDependencies": {
    "@westegg/dev": "0.0.2-0",
    "@misk/tslint": "0.1.5-18",
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.12.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.0.5",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.4",
    "@types/webpack": "^4.4.22",
    "@types/webpack-env": "^1.13.6",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.3",
